<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="seq_rank_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">rank.student</field>
            <field name="prefix">UBa19E</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_rank_lecturer" model="ir.sequence">
            <field name="name">Lecturer Sequence</field>
            <field name="code">rank.lecturers</field>
            <field name="prefix">Rank22E</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

        <!-- ================notification======================= -->
        <record id="display_notification" model="ir.actions.client">
            <field name="name">Student Created</field>
            <field name="tag">display_notification</field>
            <field name="target">new</field>
            <!-- <field name="company_id" eval="False" />s -->
            <!-- <field name="params" eval="{'message': 'hello','type':'success'" /> -->
        </record>

    </data>
</odoo>